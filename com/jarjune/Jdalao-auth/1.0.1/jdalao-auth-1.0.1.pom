                <groupId>com.jarjune</groupId>
                <artifactId>jdalao-framework-common</artifactId>
                <version>${jdalao.version}</version>
            </dependency>
            <dependency>
                <groupId>com.jarjune</groupId>
                <artifactId>jdalao-framework-util</artifactId>
                <version>${jdalao.version}</version>
            </dependency>
            <dependency>
                <groupId>com.jarjune</groupId>
                <artifactId>jdalao-framework-support</artifactId>
                <version>${jdalao.version}</version>
            </dependency>
            <dependency>
                <groupId>com.jarjune</groupId>
                <artifactId>jdalao-framework-support-redis</artifactId>
                <version>${jdalao.version}</version>
            </dependency>
            <dependency>
                <groupId>com.jarjune</groupId>
                <artifactId>jdalao-swagger</artifactId>
