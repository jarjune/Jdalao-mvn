    <modules>
        <module>jdalao-auth-api</module>
        <module>jdalao-auth-common</module>
        <module>jdalao-auth-service</module>
        <module>jdalao-auth-web</module>
        <module>jdalao-auth-admin</module>
    </modules>

</project>